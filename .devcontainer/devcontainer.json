{
    "name": "Odoo Dev Container",
    "dockerComposeFile": "docker-compose.yml",
    "service": "odoo",
    "workspaceFolder": "/workspace",
    "extensions": [
      "ms-python.python",
      "esbenp.prettier-vscode"
    ],
    "settings": {
      "terminal.integrated.defaultProfile.linux": "bash"
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "remoteUser": "vscode"
}
